<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:PresentationOptions="http://schemas.microsoft.com/winfx/2006/xaml/presentation/options"
    xmlns:reskeys="clr-namespace:MsgBox.Themes"
    >
    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="/UserNotification;component/Themes/LightBrushs.xaml" />
    </ResourceDictionary.MergedDictionaries>

    <!-- Control -->
    <SolidColorBrush x:Key="{x:Static reskeys:ResourceKeys.MsgBox_MessageColor}" Color="Black" PresentationOptions:Freeze="True" />

    <SolidColorBrush x:Key="{x:Static reskeys:ResourceKeys.MsgBoxDialog_BackgroundKey}" Color="White" />

    <SolidColorBrush x:Key="{x:Static reskeys:ResourceKeys.MsgBoxView_BackgroundKey}" Color="Transparent" />
    <SolidColorBrush x:Key="{x:Static reskeys:ResourceKeys.MsgBoxView_ForegroundKey}" Color="Black" />

</ResourceDictionary>
